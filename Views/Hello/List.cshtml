@model IEnumerable<MvcDemo.Models.Course>

@{
   //  Layout = "~/Views/Shared/common.cshtml";
    ViewBag.Title = "List Of Courses";
}
    <h2>List Of Courses</h2>
    <table border="1" style="width:100%">
        <tr>
            <th>
               Name 
            </th>
            <th>
               Duration
            </th>
        </tr>
    
    @foreach (var course in Model)
    {
        <tr>
            <td>
                @course.Name
            </td>
            <td>
                @course.Duration
            </td>
        </tr>
    }
    
    </table>

